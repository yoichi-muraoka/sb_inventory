<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.inventory.mapper.AdminMapper">

	<select id="selectByLoginId"  parameterType="string" resultType="com.example.inventory.domain.Admin">
		SELECT * FROM admins
		WHERE login_id = #{loginId};
	</select>

</mapper>






